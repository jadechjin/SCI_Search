# 检索源
SERPAPI_API_KEY=

# 模型密钥
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=

# 模型设置
LLM_PROVIDER=openai
LLM_MODEL=
LLM_BASE_URL=
LLM_TEMPERATURE=0.0
LLM_MAX_TOKENS=4096

# 检索设置
DEFAULT_MAX_RESULTS=100
SERPAPI_MAX_CALLS=
DOMAIN=general #如需自定义可以解锁下面自行填写，内置泛化STEM描述（general）以及材料学描述（materials_science）

# 自定义领域描述 (example)
# DOMAIN=makesi
# makesi=makesi is a metallurgy-focused research domain; key terms include HEA, phase diagram, diffusion, CALPHAD

# 工作流相关设置
RELEVANCE_BATCH_SIZE=10
RELEVANCE_MAX_CONCURRENCY=3
DEDUP_ENABLE_LLM_PASS=true
DEDUP_LLM_MAX_CANDIDATES=60

# MCP Session Polling（不重要可以直接忽略）
# MCP_DECIDE_WAIT_TIMEOUT_S=15.0
# MCP_POLL_INTERVAL_S=0.05
# MCP_REQUIRE_USER_RESPONSE=true
